n = int(input("Enter a number greater than 1: "))

if n <= 1:
    print("Number must be greater than 1.")
else:
    is_prime = True
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            print(n, "is not a prime (divisible by", i, ")")
            is_prime = False
            break
    if is_prime:
        print(n, "is Prime")
